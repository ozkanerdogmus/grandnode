@model IList<ProductOverviewModel>
<div class="home-page-product-grid">
    <div class="row-product">
        @foreach (var item in Model)
        {
            <div class="col-xl-2 col-lg-3 col-md-3 col-sm-6 col-6 product-container mb-3">
                <partial name="_ProductBox" model="item" />
            </div>
        }
    </div>
</div>
